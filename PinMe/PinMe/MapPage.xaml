<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PinMe.MapPage">
  <ContentPage.Content>
    <StackLayout x:Name="mapLayout" Orientation="Vertical" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" >
      <Button x:Name="RangeButton" Text="Range" TextColor="Black" HorizontalOptions="FillAndExpand" VerticalOptions="Start" BackgroundColor="White" Clicked="RangeButtonClicked"></Button>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>